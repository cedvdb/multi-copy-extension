{
	"manifest_version": 2,
  "name": "Test Extension",
	"version": "1.0",
  // Recommended
  //"default_locale": "en",
	"description": "A test extension",
	"browser_action": {
		"default_icon": "assets/icon.png",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts":[
		{
			"matches":["<all_urls>"],
			"js": ["content_script.js"]
		}
	],
	"permissions": [ "storage"]
}